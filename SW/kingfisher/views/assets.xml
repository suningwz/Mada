<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="asset_frontend" inherit_id="website.assets_frontend" name="Kingfisher assets frontend">
        <xpath expr="." position="inside">
            <!-- Libraries -->
            <link href="/kingfisher/static/src/css/owl.carousel.css" rel="stylesheet"/>
            <link href="/kingfisher/static/src/css/unite-gallery.css" rel="stylesheet"/>
            <link href='/kingfisher/static/src/skins/alexis/alexis.css' rel="stylesheet"/>
            <link href='/kingfisher/static/src/css/ion.rangeSlider.css' rel="stylesheet"/>
            <link href='/kingfisher/static/src/css/ion.rangeSlider.skinHTML5.css' rel="stylesheet"/>
            <link href='/kingfisher/static/src/css/kingfisher_pro.css' rel="stylesheet"/>

            <script src="/kingfisher/static/src/js/unitegallery.min.js" type="text/javascript"/>
            <script src="/kingfisher/static/src/js/ug-theme-compact.js" type="text/javascript"/>
            <script src="/kingfisher/static/src/js/ion.rangeSlider.min.js" type="text/javascript"/>

            <!-- Custom -->
            <!-- Theme -->
            <link href="/kingfisher/static/src/scss/options/colors/colors.scss" rel="stylesheet" type="text/scss"/>
            <link href="/kingfisher/static/src/scss/mixins.scss" rel="stylesheet" type="text/scss"/>
            <link href="/kingfisher/static/src/scss/box-layout.scss" rel="stylesheet" type="text/scss"/>
            <link href="/kingfisher/static/src/scss/header.scss" rel="stylesheet" type="text/scss"/>
            <link href="/kingfisher/static/src/scss/footer.scss" rel="stylesheet" type="text/scss"/>
            <link href="/kingfisher/static/src/scss/product.scss" rel="stylesheet" type="text/scss"/>
            <link href="/kingfisher/static/src/scss/web.scss" rel="stylesheet" type="text/scss"/> 
            
            <link href="/kingfisher/static/src/scss/style.scss" rel="stylesheet" type="text/scss"/>
            <link href="/kingfisher/static/src/scss/theme_common.scss" rel="stylesheet" type="text/scss"/>

            <script type="text/javascript" src="/kingfisher/static/src/js/owl.carousel.min.js"></script>
            <script type="text/javascript" src="/kingfisher/static/src/js/carousel-swipe.js"></script>

            <script src="/kingfisher/static/src/js/kingfisher_frontend.js" type="text/javascript"/>
            <script src="/kingfisher/static/src/js/kingfisher.js" type="text/javascript"/>
            <script src="/kingfisher/static/src/js/custom.js" type="text/javascript"/>

            <script src="/kingfisher/static/src/js/rating_state.js" type="text/javascript"/>
            

        </xpath>
    </template>

    <!-- Assets Editor -->
    <template id="assets_editor" inherit_id="web_editor.assets_editor" name="Kingfisher assets editor">
        <xpath expr="." position="inside">
            <script src="/kingfisher/static/src/js/kingfisher_editor.js" type="text/javascript"/>
        </xpath>
    </template>
    
</odoo>