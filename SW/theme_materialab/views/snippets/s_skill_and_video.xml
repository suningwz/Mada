<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template name="Skill And Video" id="s_skill_and_video">
        <!-- ====START SKILL AND VEDIO TABLE==== -->
        <section class="skill_and_video oe_structure">
            <div class="video_area">
                <div class="video_control">
                    <div class="dtable">
                        <div class="dcell">
                            <a href="https://youtu.be/yiRK8kE5Yww?rel=0&amp;autoplay=1" data-type="youtube" id="control" class="btn waves-effect waves-light veno">
                                <i class="zmdi zmdi-play"></i>
                            </a>
                            <h4>Check out the Video</h4>
                        </div>
                    </div>
                </div>
            </div>
            <div class="container fix_p">
                <div class="row">
                    <div class="col-xs-12 col-sm-offset-5 col-sm-7 section-padding skillBg">
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="all_skill_item">
                                    <div class="single_progress">
                                        <h4>E-commerce</h4>
                                        <div class="progress">
                                            <div class="progress-bar wow animated" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 95%;">
                                                <span>95%</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="single_progress">
                                        <h4>Retail ERP</h4>
                                        <div class="progress">
                                            <div class="progress-bar wow animated" role="progressbar" aria-valuenow="80" aria-valuemin="0" aria-valuemax="100" style="width: 90%;">
                                                <span>90%</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="single_progress">
                                        <h4>Inventory</h4>
                                        <div class="progress">
                                            <div class="progress-bar wow animated" role="progressbar" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100" style="width: 85%;">
                                                <span>85%</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-3 col-xs-6">
                                <div class="single_skill">
                                    <input type="text" class="knob" value="0" data-rel="90" data-width="100" data-height="100" data-bgcolor="#4073ff" data-fgcolor="#d3dfff" data-thickness=".12" data-readonly="true" data-angleOffset="0" disabled="True" />
                                    <h3>Payment Gateway</h3>
                                </div>
                            </div>
                            <div class="col-md-3 col-xs-6">
                                <div class="single_skill">
                                    <input type="text" class="knob" value="0" data-rel="80" data-width="100" data-height="100" data-bgcolor="#4073ff" data-fgcolor="#d3dfff" data-thickness=".12" data-readonly="true" data-angleOffset="0" disabled="True" />
                                    <h3>100+ Beautiful Themes</h3>
                                </div>
                            </div>
                            <div class="col-md-3 col-xs-6">
                                <div class="single_skill">
                                    <input type="text" class="knob" value="0" data-rel="90" data-width="100" data-height="100" data-bgcolor="#4073ff" data-fgcolor="#d3dfff" data-thickness=".12" data-readonly="true" data-angleOffset="0" disabled="True" />
                                    <h3>Once click installation</h3>
                                </div>
                            </div>
                            <div class="col-md-3 col-xs-6">
                                <div class="single_skill">
                                    <input type="text" class="knob" value="0" data-rel="100" data-width="100" data-height="100" data-bgcolor="#4073ff" data-fgcolor="#d3dfff" data-thickness=".12" data-readonly="true" data-angleOffset="0" disabled="True" />
                                    <h3>Own Branding</h3>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <!-- ====END SKILL AND VEDIO TABLE==== -->
    </template>
</odoo>
