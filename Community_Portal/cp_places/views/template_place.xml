<?xml version="1.0" encoding="utf-8"?>

<odoo>

    <template id="template_place">
        <title>VNMG::Place Location</title>
        <t t-call="website.layout">
            <div id="map_wrapper">
                <div id="map_canvas" class="mapping"></div>
            </div>
            <form>
                <div class="container">
                    <div class="row">
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12  text-center mt20">
                            <a href="https://goo.gl/NPfPMk" target="_blank"/>
                            <input type="hidden" t-att-value="record.place_name" id ='pdata'/>
                        </div>
                    </div>
                </div>
            </form>
        </t>
    </template>

</odoo>
