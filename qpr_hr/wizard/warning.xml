<odoo>
    <data>

        <record id="hr_leave_warning_view" model="ir.ui.view">
            <field name="name">hr.leave.warning.form</field>
            <field name="model">hr.leave.warning</field>
            <field name="arch" type="xml">
                <form string="Attachment">
                    <p class="oe_grey">
                        You have not added the attachment, please add it before confirming.
                    </p>
                    <footer>
                        <button string="Submit" name="action_submit" type="object" default_focus="1" class="btn-primary"/>
                        <button string="Cancel" class="btn-secondary" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

    </data>
</odoo>